{
  "name": "artboard-history",
  "version": "0.8.2",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "artboard-history",
    "manifest": "src/manifest.json",
    "main": "artboard-history.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jan-patrick/sketch_artboard-history.git"
  }
}
