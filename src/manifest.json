{
  "name": "Artboard History",
  "description": "Get faster to your latest Artboards",
  "author": "Jan Schneider",
  "authorEmail": "hello@jan-patrick.de",
  "homepage": "https://github.com/jan-patrick/sketch_artboard-history",
  "compatibleVersion": 54,
  "bundleVersion": 1,
  "version": "1.4.4",
  "identifier": "com.github.jan-patrick.sketch_artboard-history",
  "icon": "icon.png",
  "suppliesData": false,
  "commands": [
    {
      "identifier": "ArtboardHistory_goToLastArtboard",
      "script": "commands.js",
      "shortcut": "cmd alt a",
      "handler": "goToLastArtboard",
      "description": "Go to the Last Artboard.",
      "icon": "icons/goBackinArtboardHistory.png",
      "name": "Go to Last Artboard"
    },
    {
      "identifier": "ArtboardHistory_showSelectedLayerInfo",
      "script": "commands.js",
      "shortcut": "cmd alt l",
      "handler": "showSelectedLayerInfo",
      "description": "Get info about one selected layer.",
      "icon": "icons/showSelectedLayerInfo.png",
      "name": "DEV info"
    },
    {
      "identifier": "ArtboardHistory_showSavedArtboardHistory",
      "script": "commands.js",
      "shortcut": "cmd alt p",
      "handler": "showSavedArtboardHistory",
      "description": "Show the saved Artboard History.",
      "icon": "icons/showSelectedLayerInfo.png",
      "name": "Show the saved Artboard History"
    },
    {
      "identifier": "ArtboardHistory_setZoomSetting",
      "script": "commands.js",
      "shortcut": "cmd alt m",
      "handler": "setZoomSetting",
      "description": "Set if zoom to Artboard or not.",
      "icon": "icons/showSelectedLayerInfo.png",
      "name": "Set if zoom to Artboard or not"
    },
    {
      "script": "commands.js",
      "name": "Update last Artboard",
      "handlers": {
        "actions": {
          "ArtboardChanged.finish": "updateArtboardHistory"
        }
      },
      "identifier": "ArtboardHistory-action-listener-identifier"
    },
    {
      "identifier": "ArtboardHistory_ResetAllSettings",
      "script": "commands.js",
      "shortcut": "cmd alt r",
      "handler": "resetAllSetSettings",
      "description": "Clear all saved data in Settings.",
      "icon": "icons/showSelectedLayerInfo.png",
      "name": "Clear all saved data in Settings"
    },
    {
      "script": "commands.js",
      "name": "ckeck if ArtboardHistory already exists and is saved",
      "handlers": {
        "actions": {
          "Startup": "checkIfAllThisExists"
        }
      },
      "identifier": "ArtboardHistory-action-listener-identifier-StartUp"
    }
  ],
  "menu": {
    "title": "Artboard History",
    "items": [
      "ArtboardHistory_goToLastArtboard", 
      "ArtboardHistory_setZoomSetting",
      "ArtboardHistory_ResetAllSettings",
      "ArtboardHistory_showSavedArtboardHistory"
    ]
  }
}