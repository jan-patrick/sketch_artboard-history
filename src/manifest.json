{
  "name": "Artboard History",
  "description": "Get faster to your latest artboard",
  "author": "Jan Schneider",
  "authorEmail": "hello@jan-patrick.de",
  "homepage": "https://github.com/jan-patrick/sketch_artboard-history",
  "compatibleVersion": 53,
  "bundleVersion": 1,
  "version": "1.1.0",
  "identifier": "com.github.jan-patrick.sketch_artboard-history",
  "icon": "icon.png",
  "suppliesData": false,
  "commands": [
    {
      "identifier": "ArtboardHistory_goToLastArtboard",
      "script": "commands.js",
      "shortcut": "cmd alt z",
      "handler": "goToLastArtboard",
      "description": "Go to the Last Artboard.",
      "icon": "icons/goBackinArtboardHistory.png",
      "name": "Go to Last Artboard"
    },
    {
      "identifier": "ArtboardHistory_showSelectedLayerInfo",
      "script": "commands.js",
      "shortcut": "cmd alt i",
      "handler": "showSelectedLayerInfo",
      "description": "Get info about one selected layer.",
      "icon": "icons/showSelectedLayerInfo.png",
      "name": "Get info of selected layer"
    },
    {
      "script": "commands.js",
      "name": "Update last Artboard",
      "handlers": {
        "actions": {
          "ArtboardChanged.finish": "updateArtboardHistory"
        }
      },
      "identifier": "ArtboardHistory-action-listener-identifier"
    }
  ],
  "menu": {
    "title": "Artboard History",
    "items": [
      "ArtboardHistory_goToLastArtboard",
      "ArtboardHistory_showSelectedLayerInfo"
    ]
  }
}